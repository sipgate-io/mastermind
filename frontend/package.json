{
  "name": "mastermind-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start >/dev/null & (cd ../backend && (npm run start & node build/API_Controller.js >/dev/null))",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll=false",
    "test:frontend": "react-scripts test --watchAll=false App.test.tsx",
    "test:integration": "react-scripts test --watchAll=false integration.test.tsx",
    "eject": "react-scripts eject",
    "preinstall": "git config core.hooksPath ../.git-hooks"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.62",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.4.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4",
    "web-vitals": "^2.1.4",
    "websocket": "^1.0.34"
  },
  "devDependencies": {
    "@types/websocket": "^1.0.5",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{ts,tsx}": "prettier --write"
  }
}
